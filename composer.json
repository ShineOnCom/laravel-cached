{
    "name": "dan/laravel-cached",
    "description": "Your Laravel models, decorated, cached, and pleasantly accessible.",
    "license": "MIT",
    "keywords": ["laravel"],
    "type": "project",
    "authors": [
        {
            "name": "Dan Richards",
            "email": "danrichardsri@gmail.com"
        }
    ],
    "autoload" : {
        "psr-4": {
            "More\\Laravel\\Cached\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "require": {
        "php": ">=7.1",
        "illuminate/cache": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/database": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "6.2|^7.0|^8.0|^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "More\\Laravel\\Cached\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit ./tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "More\\Laravel\\Cached\\Providers\\CachedServiceProvider"
            ]
        }
    }
}
