{
    "name": "dan/laravel-cached",
    "description": "Your Laravel models, decorated, cached, and pleasantly accessible.",
    "license": "MIT",
    "keywords": ["laravel"],
    "type": "project",
    "authors": [
        {
            "name": "Dan Richards",
            "email": "danrichardsri@gmail.com"
        }
    ],
    "autoload" : {
        "psr-4": {
            "More\\Laravel\\Cached\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "require": {
        "php": ">=5.6",
        "illuminate/cache": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/database": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/support": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "More\\Laravel\\Cached\\Support\\CachedServiceProvider"
            ]
        }
    }
}
